CREATE TABLE Person(
	id BIGINT(20) PRIMARY KEY AUTO_INCREMENT,
	firstName VARCHAR(30) NOT NULL,
	lastName VARCHAR(30) NOT NULL,
	street VARCHAR(50),
	city VARCHAR(50),
	state VARCHAR(50),
	postalCode VARCHAR(50)
)ENGINE=InnoDB DEFAULT CHARSET=utf8;


